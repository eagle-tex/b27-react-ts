{
  "name": "b27-react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cz": "git cz",
    "format": "npx prettier --write \"**/*.+(cjs|js|jsx|ts|tsx|json|yml|yaml|css|scss|md|pug|vue|log)\"",
    "lint:fix": "eslint . --fix --ext .cjs,.js,.jsx,.ts,.tsx, --ignore-path .eslintignore",
    "lint:second": "eslint --fix src test",
    "prepare": "husky install",
    "prettier:check": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "stylelint:check": "stylelint \"**/*.css\"",
    "stylelint:fix": "stylelint --fix \"**/*.css\"",
    "test": "vitest",
    "test:vitest": "DEBUG=test DEBUG_DEPTH=6 DEBUG_COLORS=true cross-env NODE_ENV=test vitest --watch --reporter verbose",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@heroicons/react": "^2.0.18",
    "@mui/icons-material": "^5.14.18",
    "@mui/material": "^5.14.18",
    "@testing-library/user-event": "^14.5.1",
    "axios": "^1.6.2",
    "debug": "^4.3.4",
    "framer-motion": "^10.16.4",
    "i18next": "^23.7.6",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.5.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/ui": "^0.34.6",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "msw": "^2.0.9",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^4.1.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "msw": {
    "workerDirectory": "./public"
  }
}
